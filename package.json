{
  "name": "@azesmway/react-native-rutube",
  "version": "0.1.9",
  "description": "React Native Rutube plugin",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "cpp",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "bootstrap": "yarn"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "rutube"
  ],
  "repository": "https://github.com/azesmway/react-native-rutube",
  "author": "azesm <azesm@me.com> (https://github.com/azesmway)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azesmway/react-native-rutube/issues"
  },
  "homepage": "https://github.com/azesmway/react-native-rutube#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@types/react-native": "^0.67.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-native": ">=0.67.4",
    "react-native-web-webview": "^1.0.2",
    "react-native-webview": "^11.18.1"
  },
  "peerDependenciesMeta": {
    "react-native-web-webview": {
      "optional": true
    },
    "react-native-webview": {
      "optional": true
    }
  },
  "dependencies": {
    "events": "^3.3.0",
    "react-native-web-webview": "^1.0.2",
    "react-native-webview": "^11.18.1"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true,
      "access": "public"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
